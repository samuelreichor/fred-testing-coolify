{
  "require": {
    "craftcms/ckeditor": "4.10.1",
    "craftcms/cms": "5.8.17",
    "craftcms/commerce": "5.4.6",
    "craftcms/mailgun": "3.1.0",
    "doublesecretagency/craft-cpcss": "3.0.0",
    "nystudio107/craft-retour": "5.0.12",
    "nystudio107/craft-seomatic": "5.1.17",
    "presseddigital/linkit": "5.0.0",
    "putyourlightson/craft-campaign": "3.6.0",
    "samuelreichor/craft-query-api": "dev-dev",
    "samuelreichor/craft-quick-edit": "5.2.2",
    "simplygoodwork/craft-block-usage": "dev-craft-5 as 2.0.1",
    "spacecatninja/imager-x": "5.1.6",
    "studioespresso/craft-dumper": "5.0.1",
    "verbb/image-resizer": "4.0.4",
    "vlucas/phpdotenv": "5.6.2"
  },
  "autoload": {
    "psr-4": {
      "modules\\queryapiextensions\\": "modules/query-api-extensions/",
      "modules\\hrpreview\\": "modules/hr-preview/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true,
      "php-http/discovery": true
    }
  },
  "scripts": {
    "generate-types": "php craft query-api/typescript/generate-types --output=@root/frontend/shared/types/base.ts",
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "create-folders": [
      "mkdir -p ./storage",
      "mkdir -p ./config/project",
      "mkdir -p ./web/cpresources",
      "mkdir -p ./web/assets"
    ],
    "pre-craft-update": [
      "@create-folders"
    ],
    "post-craft-update": [
      "@php craft install/check && php craft clear-caches/compiled-templates || exit 0",
      "@php craft install/check && php craft clear-caches/data || exit 0",
      "@php craft install/check && php craft clear-caches/temp-files || exit 0",
      "@php craft install/check && php craft up || exit 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome"
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "https://github.com/samuelreichor/craft-block-usage"
    }
  ],
  "name": "vendor/name",
  "description": ""
}
