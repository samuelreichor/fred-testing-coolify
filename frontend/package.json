{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nuxt dev -o --host",
    "build": "nuxt build && npm run build:cp",
    "dev": "rm -rf ./frontend/.output && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview --port 4000",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxi typecheck",
    "ci": "npm run postinstall && npm run typecheck && npm run lint",
    "build:cp": "npx @tailwindcss/cli -i ./app/assets/css/cp.css -o ../web/static/css/cp.css",
    "dev:cp": "npx @tailwindcss/cli -i ./app/assets/css/cp.css -o ../web/static/css/cp.css --watch"
  },
  "dependencies": {
    "@blossom-carousel/vue": "^0.1.9",
    "@nuxtjs/color-mode": "^3.5.2",
    "@query-api/nuxt": "0.7.1",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "lazysizes": "^5.3.2",
    "lenis": "^1.3.11",
    "nuxt": "^4.1.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.1.0",
    "@tailwindcss/vite": "^4.0.6",
    "@hypernym/nuxt-gsap": "^2.4.3",
    "@types/node": "^24.3.1",
    "class-variance-authority": "^0.7.0",
    "eslint": "^9.21.0",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6",
    "vite-svg-loader": "^5.1.0",
    "tailwindcss": "^4.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
